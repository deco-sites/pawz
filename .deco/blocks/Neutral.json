{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--btn-focus-scale": "0.95",
    "--animation-btn": "0.25s"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "mainColors": {
    "base-100": "#f4f4f4",
    "primary": "#ffffff",
    "secondary": "#121212",
    "tertiary": "#000000",
    "neutral": "#000000",
    "success": "#25D366",
    "warning": "#E28759",
    "error": "#FC4F63",
    "info": "#009AFF"
  },
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "300"
          },
          {
            "weight": "400"
          },
          {
            "weight": "500"
          },
          {
            "weight": "700"
          }
        ],
        "family": "Roboto"
      }
    ]
  },
  "mode": "light",
  "complementaryColors": {
    "base-200": "#E2E2E2",
    "base-300": "#C7C7C7",
    "base-400": "#666666",
    "base-500": "#353535"
  }
}
